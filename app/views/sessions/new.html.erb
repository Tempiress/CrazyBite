<div class="container">
  <h2 class="title"> Вход</h2>

  <p class="mt"></p>


  <div class="mt">

    <%= form_with scope: :session, url: session_path do |form| %>


      <p><%= form.label :email, tittle: "email", class: 'title-sm'%></p>
      <p><%= form.text_field :email, class: 'btn_edit_q' %></p>

      <p class="mt"><%= form.label :password, tittle:"password", class: 'title-sm'%></p>
      <p><%= form.password_field :password, class: 'btn_edit_q' %></p>


      <p class="mt-lg"><%= form.submit 'Войти', class: 'BtnQ ' %></p>
    <% end %>
  </div>


</div>